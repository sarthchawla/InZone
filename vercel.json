{
  "buildCommand": "pnpm run build",
  "outputDirectory": "apps/web/dist",
  "rewrites": [
    { "source": "/api/auth/:path*", "destination": "/api" },
    { "source": "/api/:path*", "destination": "/api" }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    }
  },
  "git": {
    "deploymentEnabled": false
  }
}
